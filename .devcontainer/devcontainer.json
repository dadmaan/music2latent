{
    "name": "music2latent Dev Container",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "music2latent",
    "workspaceFolder": "/app",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-toolsai.jupyter"
            ],
            "settings": {
                "python.defaultInterpreterPath": "/opt/conda/bin/python"
            }
        }
    }
}